@{
    Layout = "_SearchLayout";
}
@using DASoTiemChung.Controllers
@using DASoTiemChung.Dtos

@{
    int i = 1;
    var thongtinTraCuu = ViewBag.ThongTinTraCuu as ThongTinTiemChungOutputDto;
}

@section Styles {
    <style type="text/css">
    </style>
}



    <div class="container" style="margin-top:80px">
        <div class="page-heading">
            <div class="page-title">
                <div class="row">
                    <div class="col-12 col-md-6 order-md-1 order-last">
                        <h3>@Html.Raw(ViewBag.Title)</h3>

                    </div>
                    <div class="col-12 col-md-6 order-md-2 order-first">
                        <nav aria-label="breadcrumb" class="breadcrumb-header float-start float-lg-end">
                            <ol class="breadcrumb">
                                @*<li class="breadcrumb-item"><a href="javascript:;">Dữ liệu cơ bản (v2)</a></li>*@
                                @*<li class="breadcrumb-item active" aria-current="page">Quản lý trường học (v2)</li>*@
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
            <section class="section">
                <div class="card border">
                    <div class="card-header d-flex align-items-center">
                        <h4 class="card-title m-0">Tra cứu chứng nhận tiêm</h4>

                    </div>
                    <div class="card-content show" id="search">
                        <div class="card-body">
                            <form id="searchForm" class="form" asp-controller="TraCuuTiemChung" asp-action="Index" method="get">
                                <div class="row">
                                    <div class="col-md-3 col-12">
                                        <div class="form-group">
                                            <label for="searchTenNguoiDan">Tên người dân</label>
                                            <div class="position-relative">
                                                <input type="text" id="searchTenNguoiDan" class="form-control"
                                                   placeholder="Nhập tên người dân muốn tìm" name="TenNguoiDan">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-3 col-12">
                                        <div class="form-group">
                                            <label for="searchSoCccdhc">Số căn cước công dân</label>
                                            <div class="position-relative">
                                                <input type="text" id="searchSoCccdhc" class="form-control"
                                                   placeholder="Nhập số căn cước công dân muốn tìm" name="SoCCCDHC">
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-12 d-flex justify-content-end">
                                        <button type="submit" id="btnCheckEmpty"
                                            class="btn btn-primary me-1 mb-1">
                                            Tra cứu
                                        </button>
                                        @*<button type="reset"
                                    class="btn btn-light-secondary me-1 mb-1">
                                    Reset
                                    </button>*@
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Striped rows start -->
            <section class="section">
                <div class="container">
                    @if (@TempData["Message"] != null)
                {
                    <div class="text-danger text-center">@TempData["Message"]</div>
                }
                else
                {
                    <div id="dataTable" style="min-height: 100px;">

                        <div _ngcontent-lww-c8="" class="row ng-star-inserted">
                            <div _ngcontent-lww-c8="" class="col-lg-9">
                                <div _ngcontent-lww-c8="" class="w-100 text-center">
                                    <p _ngcontent-lww-c8="" class="text-uppercase fw-bolder mb-1">
                                        CỘNG HÒA XÃ HỘI CHỦ NGHĨA VIỆT NAM
                                    </p>
                                    <p _ngcontent-lww-c8="" class="fw-bolder">
                                        Độc lập - Tự do - Hạnh phúc
                                    </p>
                                </div>
                                <div _ngcontent-lww-c8="" class="w-100 mt-4 text-center mb-4">
                                    <h4 _ngcontent-lww-c8="" class="fw-bolder">
                                        CHỨNG NHẬN TIÊM CHỦNG COVID-19
                                    </h4>
                                </div>
                                <div _ngcontent-lww-c8="" class="row">
                                    <div _ngcontent-lww-c8="" class="col-lg-4 col-md-6">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">Họ và tên</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">@thongtinTraCuu.HoTen</p>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-lg-4 col-md-6">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">Ngày sinh</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">@thongtinTraCuu.NgaySinh.ToString("dd/MM/yyyy")</p>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-lg-4 col-md-6">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">CCCD/HC</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">
                                            @thongtinTraCuu.CCCD
                                        </p>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-lg-4 col-md-6">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">Số thẻ BHYT</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">@thongtinTraCuu.BHYT</p>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-lg-4 col-md-6">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">Số điện thoại</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">@thongtinTraCuu.SoDienThoai</p>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-12">
                                        <p _ngcontent-lww-c8="" class="mb-1 fw-semibold">Địa chỉ</p>
                                        <p _ngcontent-lww-c8="" class="fw-bolder text-dark">
                                           @thongtinTraCuu.DiaChi
                                        </p>
                                    </div>
                                    
                                    <div _ngcontent-lww-c8="" class="col-12 table-responsive">
                                        <table _ngcontent-lww-c8="" class="table table-bordered table-hover">
                                            <thead _ngcontent-lww-c8="">
                                                <tr _ngcontent-lww-c8="" class="bg-light text-dark">
                                                    <th _ngcontent-lww-c8="" class="text-center">Mũi số</th>
                                                    <th _ngcontent-lww-c8="" class="text-center">
                                                        Thời gian tiêm
                                                    </th>
                                                    <th _ngcontent-lww-c8="" class="text-center">
                                                        Tên vắc xin
                                                    </th>
                                                    <th _ngcontent-lww-c8="" class="text-center">Bác sỹ tiêm</th>
                                                    <th _ngcontent-lww-c8="" class="text-center">Nơi tiêm</th>
                                                </tr>
                                            </thead>
                                            <tbody _ngcontent-lww-c8="">
                                                <!---->
                                                @foreach (var combo in ViewBag.GetPhieuTiems)
                                            {
                                                <tr _ngcontent-lww-c8="" class="ng-star-inserted">
                                                    <td _ngcontent-lww-c8="" class="text-center" scope="row">
                                                            @Html.Raw(combo.MaMuiTiem)
                                                    </td>
                                                    <td _ngcontent-lww-c8="" class="text-center">
                                                            @Html.Raw(combo.ThoiGianTiem)
                                                    </td>
                                                    <td _ngcontent-lww-c8="" class="text-center">
                                                       @Html.Raw(combo.MaVacXinTheoLoNavigation?.TenVacXinTheoLo)
                                                    </td>
                                                    <td _ngcontent-lww-c8="" class="text-center">
                                                            @Html.Raw(combo.MaNhanVienNavigation?.TenNhanVien)
                                                    </td>
                                                    <td _ngcontent-lww-c8="" class="text-center">
                                                            @Html.Raw(combo.MaKhoNavigation?.TenKho)
                                                    </td>
                                                </tr>
                                            }
                                            </tbody>
                                        </table>
                                    </div>
                                    <!---->
                                </div>
                            </div>
                            <div _ngcontent-lww-c8="" class="col-lg-3 rounded-3 p-4 shadow-lg h-100 bg-success ">
                                <div _ngcontent-lww-c8="" class="row">

                                    <div _ngcontent-lww-c8="" class="col-12 text-center text-white mt-2 mb-2 pt-2 pb-2 pl-0 pr-0">
                                        <h4 _ngcontent-lww-c8="" class="text-uppercase fw-bolder">
                                            Quét mã QR thông tin tiêm chủng
                                        </h4>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-12 text-center mb-3 p-2">
                                        <ngx-qrcode _ngcontent-lww-c8="">
                                            <div class="qrcode">
                                                <img src="data:image/png;base64," />
                                            </div>
                                        </ngx-qrcode>
                                    </div>
                                    <div _ngcontent-lww-c8="" class="col-12">
                                        <div _ngcontent-lww-c8="" class="w-100 p-3 bg-white radius20">
                                            <div _ngcontent-lww-c8="" class="d-flex text-dark">
                                                <div _ngcontent-lww-c8="" style="width: 30px">
                                                    <i _ngcontent-lww-c8="" class="icofont-business-man"></i>
                                                </div>
                                                <div _ngcontent-lww-c8="" class="w-100">
                                                    <p _ngcontent-lww-c8="" class="mb-1">Họ và tên</p>
                                                    <p _ngcontent-lww-c8="" class="fw-bolder text-dark">
                                                        @thongtinTraCuu.HoTen
                                                    </p>
                                                </div>
                                            </div>
                                            <div _ngcontent-lww-c8="" class="d-flex text-dark">
                                                <div _ngcontent-lww-c8="" style="width: 30px">
                                                    <i _ngcontent-lww-c8="" class="icofont-birthday-cake"></i>
                                                </div>
                                                <div _ngcontent-lww-c8="" class="w-100">
                                                    <p _ngcontent-lww-c8="" class="mb-1">Ngày sinh</p>
                                                    <p _ngcontent-lww-c8="" class="fw-bolder">@thongtinTraCuu.NgaySinh</p>
                                                </div>
                                            </div>
                                            <div _ngcontent-lww-c8="" class="d-flex text-dark">
                                                <div _ngcontent-lww-c8="" style="width: 30px">
                                                    <i _ngcontent-lww-c8="" class="icofont-ui-v-card"></i>
                                                </div>
                                                <div _ngcontent-lww-c8="" class="w-100">
                                                    <p _ngcontent-lww-c8="" class="mb-1">CCCD/HC</p>
                                                    <p _ngcontent-lww-c8="" class="fw-bolder">@thongtinTraCuu.CCCD</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                }




            </div>
        </section>
        <!-- Striped rows end -->
    </div>
    <div class="modal fade" id="detailModal" tabindex="-1" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
            </div>
        </div>
    </div>
</div>


@section Scripts {
    
    
}